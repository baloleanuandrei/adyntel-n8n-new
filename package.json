{
  "name": "n8n-nodes-adyntel",
  "version": "0.1.0",
  "description": "n8n community node to integrate with Adyntel's API. Get comprehensive Facebook, Google and LinkedIn advertising data including ads, copy and creatives.",
  "keywords": [
    "n8n-community-node-package",
    "adyntel",
    "facebook",
    "facebook ads",
    "google",
    "google ads",
    "linkedin",
    "linkedin ads",
    "advertising",
    "ad intelligence",
    "marketing",
    "ad intelligence",
    "ad intelligence api",
    "competitive intelligence",
    "competitive intelligence api",
    "competitive analysis",
    "competitive research",
    "competition research",
    "competition analysis",
    "competition research api",
    "competition analysis api",
    "competition research api",
    "social media"
  ],
  "license": "MIT",
  "homepage": "https://github.com/adyntel/n8n-nodes-adyntel#readme",
  "author": {
    "name": "Adyntel",
    "email": "hello@adyntel.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adyntel/n8n-nodes-adyntel.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AdyntelApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Adyntel/Adyntel.node.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/adyntel/n8n-nodes-adyntel/issues"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.5",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "task-master-ai": "^0.28.0"
  }
}
