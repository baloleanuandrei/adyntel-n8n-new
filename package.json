{
  "name": "n8n-nodes-adyntel",
  "version": "0.1.8",
  "description": "n8n node to call Adyntel's API services. Find out who's running ads on Meta, Linkedin or Google.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://adyntel.com",
  "author": {
    "name": "adyntel",
    "email": "hello@adyntel.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baloleanuandrei/adyntel-n8n-new"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\" && tsc && gulp build:icons && node -e \"const fs=require('fs');const f='dist/credentials/AdyntelApi.credentials.js';let c=fs.readFileSync(f,'utf8');c=c.replace(/this\\.helpers\\.request/g,'this.helpers.httpRequest');fs.writeFileSync(f,c);\"",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AdyntelApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Adyntel/Adyntel.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.18.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jsonc-eslint-parser": "^2.4.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.52.0"
  },
  "overrides": {
    "axios": "^1.7.9",
    "form-data": "^4.0.1",
    "n8n-workflow": {
      "axios": "^1.7.9",
      "form-data": "^4.0.1"
    },
    "@typescript-eslint/utils": "^8.52.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
